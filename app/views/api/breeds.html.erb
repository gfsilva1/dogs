<div class="banner" style="background-image: linear-gradient(rgba(0,0,0,0.4),rgba(0,0,0,0.4)), url(https://images.unsplash.com/photo-1548199973-03cce0bbc87b?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=2069&q=80);">

  <div class="container">
    <h1>Dog breeds for you</strong>!</h1>
    <p>Just search and learn!</p>
    <a class="btn btn-flat" href="#">GO</a>
  </div>
</div>


<table>
  <thread>
    <td>Nome</td>
    <td>Expectativa de vida</td>
    <td>Temperamento</td>
    <td>Origem</td>
  </thread>

  <tbody>
    <% @breed.each do |breed|  %>
      <tr>
        <td><%= breed["name"]%></td>
        <td><%= breed["life_span"] %></td>
        <td><%= breed["temperament"] %></td>
        <td><%= breed["origin"] %></td>
      </tr>
    <% end %>
  </tbody>

</table>
